<div class="modal fade" id="createIngresoModal" tabindex="-1" aria-labelledby="createIngresoModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="createIngresoModalLabel">Crear Ingreso</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form id="createIngresoForm">
                    <div class="form-group">
                        <label for="Nombre">Nombre</label>
                        <input type="text" class="form-control" id="Nombre" name="Nombre" required>
                    </div>
                    <div class="form-group">
                        <label for="Valor">Valor</label>
                        <input type="number" class="form-control" id="Valor" name="Valor" required>
                    </div>
                    <div class="form-group">
                        <label for="FechaRegistro">Fecha de Registro</label>
                        <input type="date" class="form-control" id="FechaRegistro" name="FechaRegistro" required>
                    </div>
                    <div class="form-group">
                        <label for="FechaFin">Fecha de Fin</label>
                        <input type="date" class="form-control" id="FechaFin" name="FechaFin">
                    </div>
                    <div class="form-group">
                        <label for="IdTipoIngreso">Tipo de Ingreso</label>
                        <select class="form-control" id="IdTipoIngreso" name="IdTipoIngreso" required>
                            <option value="1">Único</option>
                            <option value="2">Mensual</option>
                        </select>
                    </div>
                    <button type="submit" class="btn btn-primary">Guardar</button>
                </form>
            </div>
        </div>
    </div>
</div>
